{
  "name": "lru2",
  "version": "0.1.3",
  "description": "Yet another LRU module with a very simple API. Just get/set and no more.",
  "main": "index.js",
  "scripts": {
    "beautify": "esbeautifier './index.js' './specs/**/*.js'",
    "beautify-check": "esbeautifier -k './index.js' './specs/**/*.js'",
    "eslint": "eslinter './index.js' './specs/**/*.js'",
    "lint": "npm run beautify && npm run eslint",
    "test": "mocha-runner './specs/**/*.js'",
    "cover": "istanbul cover -x specs/**/*.js mocha-runner './specs/**/*.js' html text-summary -- --useCache=true",
    "watch": "watch-spawn -i -p specs/**/*.js -p index.js npm run cover",
    "check": "npm run beautify-check && npm run eslint",
    "prepush": "npm run check && npm run test",
    "changelog": "changelogx -f markdown -o ./changelog.md",
    "do-changelog": "npm run changelog && git add ./changelog.md && git commit -m 'DOC: Generate changelog'",
    "install-hooks": "prepush install && changelogx install-hook",
    "bump": "bumpery --verify='npm run check'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/royriojas/lru2"
  },
  "license": "MIT",
  "author": "Roy Riojas (http://royriojas.com)",
  "prepush": [
    "npm run check"
  ],
  "devDependencies": {
    "bumpery": "^1.0.7",
    "changelogx": "^1.0.13",
    "esbeautifier": "10.1.1",
    "eslinter": "^2.0.1",
    "istanbul": "^0.3.17",
    "mocha-runner": "2.0.0",
    "prepush": "^2.0.4",
    "proxyquire": "^1.6.0",
    "watch-spawn": "^1.0.3"
  },
  "dependencies": {},
  "changelogx": {
    "ignoreRegExp": [
      "BLD: Release",
      "DOC: Generate Changelog",
      "Generated Changelog",
      "REF: formatted missing files"
    ],
    "issueIDRegExp": "#(\\d+)",
    "commitURL": "https://github.com/royriojas/lru2/commit/{0}",
    "authorURL": "https://github.com/{0}",
    "issueIDURL": "https://github.com/royriojas/lru2/issues/{0}",
    "projectName": "lru2"
  },
  "bugs": {
    "url": "https://github.com/royriojas/lru2/issues"
  },
  "homepage": "https://github.com/royriojas/lru2",
  "keywords": [
    "LRU",
    "cache",
    "LRU data structure",
    "simple LRU data structure"
  ]
}
